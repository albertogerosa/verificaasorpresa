{
    "name": "albertogerosa/verificaasorpresa",
    "description": "API REST con Slim Framework per query su database Forniture",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
